{
  "name": "cpp",
  "version": "0.0.1",
  "private": false,
  "bin": {
    "$": "./dist/bin/app.js"
  },
  "main": "./dist/app",
  "readme": "",
  "scripts": {
    "build": "node --max-old-space-size=4096 node_modules/typescript/lib/tsc.js -b",
    "watch": "tsc-watch",
    "ci:unit": "cd dist/ && mocha --recursive test/unit/**/*.js",
    "ci:all": "./node_modules/.bin/mocha test/unit/*.js test/thirdApi/*.js test/game/*.js -s 1000 --exit",
    "ci:server": "cd dist/ && mocha --recursive test/servers/**/*.js",
    "start": "cd dist/ && node config/serverTokenBuilderDev.js && pinus start -e development",
    "start:production": "cd dist/ && node config/serverTokenBuilder.js && pinus start -e production",
    "pm2": "cd dist/ && node config/serverTokenBuilderDev.js && node tools/pm2/pm2boot.js argroot=. cfgenv=dev env=development && pm2 restart pm2_development.json",
    "pm2:production": "cd dist/ && node config/serverTokenBuilder.js && node tools/pm2/pm2boot.js argroot=. cfgenv=prod env=production && pm2 reload pm2_production.json",
    "list": "cd dist/ && pinus list",
    "stop": "cd dist/ && pinus stop"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/swagger": "^5.2.1",
    "@types/bluebird": "3.5.19",
    "@types/mongoose": "5.3.26",
    "@types/node": "10.17.0",
    "@types/redlock": "3.0.2",
    "axios": "^0.26.1",
    "big.js": "^6.2.1",
    "bluebird": "3.5.1",
    "body-parser": "1.18.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.2",
    "crypto": "1.0.1",
    "csv": "^5.5.3",
    "express": "4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^3.21.3",
    "ioredis": "^4.27.2",
    "jszip": "^3.7.1",
    "mathjs": "^10.4.3",
    "mocha": "^6.2.3",
    "mongoose": "^5.12.5",
    "mysql": "^2.18.1",
    "node-schedule": "1.3.2",
    "node-watch": "0.7.3",
    "node-xlsx": "^0.17.2",
    "pinus": "1.6.3",
    "pinus-http-plugin": "^1.0.22",
    "querystring": "0.2.0",
    "ramda": "0.26.1",
    "redlock": "3.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "sha1": "1.1.1",
    "source-map-support": "^0.5.19",
    "string-format": "2.0.0",
    "swagger-ui-express": "^4.1.5",
    "ts-node": "8.6.2",
    "typeorm": "^0.2.36",
    "typescript": "^4.0.8",
    "urlencode": "1.1.0",
    "uuid": "^8.1.0",
    "ws": "^7.5.7",
    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@types/express": "4.17.2",
    "@types/express-serve-static-core": "4.17.2",
    "@types/helmet": "0.0.46",
    "@types/ioredis": "4.0.17",
    "@types/node-schedule": "1.2.4",
    "@types/ramda": "0.26.8",
    "@types/string-format": "2.0.0",
    "@types/urlencode": "1.1.1",
    "@types/ws": "^7.2.6",
    "@types/xml2js": "0.4.4",
    "chai": "^4.3.6",
    "crypto-js": "^4.0.0",
    "eslint": "8.10.0",
